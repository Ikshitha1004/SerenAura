        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.sahiti9.Serenaura"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fced1721-dd24-4da7-8110-f40ccb51fb1e"
      }
    }
  }
}
